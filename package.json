{
  "private": true,
  "name": "dumi-lib",
  "scripts": {
    "dev": "dumi dev",
    "build": "dumi build",
    "lib:build": "father-build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "predeploy": "dumi build",
    "deploy": "gh-pages -d dist"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.5",
    "father-build": "^1.17.2",
    "gh-pages": "^2.2.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-monaco-editor": "^0.35.0",
    "react-resize-detector": "^4.2.1",
    "react-split-pane": "^0.1.89",
    "yorkie": "^2.0.0"
  },
  "repository": {
    "url": "https://github.com/Nebula-Vis/Nebula-Vis.github.io.git"
  }
}
